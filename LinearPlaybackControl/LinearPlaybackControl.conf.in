precondition = ["Platform"]
autostart = "@PLUGIN_LINEAR_PLAYBACK_CONTROL_AUTOSTART@"
startuporder = "@PLUGIN_LINEARPLAYBACKCTL_STARTUPORDER@"

configuration = JSON()
rootobject = JSON()

rootobject.add("outofprocess", "true")
rootobject.add("mode", "@PLUGIN_LINEAR_PLAYBACK_CONTROL_MODE@")
rootobject.add("locator", "lib@PLUGIN_IMPLEMENTATION@.so")
rootobject.add("mountpoint", "/mnt/streamfs")
rootobject.add("streamfs_enabled", "true")

configuration.add("root", rootobject)
